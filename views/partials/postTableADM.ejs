<%- console.log("USERTABLE:",JSON.stringify(dados)) %>
<table class="table">
  <thead>
    <tr>
      <th>ID</th>
      <th>ID User</th>
      <th>Username</th>
      <th>Title</th>
      <th>Data Posted</th>
    </tr>
  </thead>
  <tbody>
    <% let i;for(i=0; i<dados.length; i++) { %>
    <tr>
      <td><%= dados[i].id_post %></td>
      <td><%= dados[i].id_users %></td>
      <td><%= dados[i].username %></td>
      <td><%= dados[i].title %></td>
      <td><%= dados[i].dateCreated %></td>
    </tr>
    <% } %>
  </tbody>
  <tfoot>
    <tr>
      <th colspan="3">
        <%- `Qtde de registros ${dados.length}` %>
      </th>
    </tr>
  </tfoot>
</table>